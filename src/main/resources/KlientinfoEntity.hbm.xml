<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Congratulator.Entity.KlientinfoEntity" table="klientinfo" schema="dbo" catalog="">
        <id name="id">
            <column name="id" sql-type="nvarchar(50)" length="50"/>
        </id>
        <property name="deleted">
            <column name="Deleted" sql-type="bit" not-null="true"/>
        </property>
        <property name="famaly">
            <column name="famaly" sql-type="nvarchar(200)" length="200" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="nvarchar(200)" length="200" not-null="true"/>
        </property>
        <property name="dateOld">
            <column name="dateOld" sql-type="datetime" not-null="true"/>
        </property>
        <property name="saveDateOld">
            <column name="saveDateOld" sql-type="int" not-null="true"/>
        </property>
        <property name="noSms">
            <column name="NoSMS" sql-type="bit" not-null="true"/>
        </property>
        <property name="name2">
            <column name="name2" sql-type="nvarchar(200)" length="200" not-null="true"/>
        </property>
        <property name="famaly2">
            <column name="famaly2" sql-type="nvarchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>